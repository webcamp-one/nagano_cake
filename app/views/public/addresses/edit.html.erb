<h2>配送先編集</h2>

<%= form_with model: @address do |f| %>
<div>
  <label>郵便番号(ハイフンなし)</label>
  <%= f.text_field :postal_code %>
</div>
<div>
  <label>住所</label>
  <%= f.text_area :address %>
</div>
<div>
  <label>宛名</label>
  <%= f.text_field :name %>
  <%= f.submit "変更内容を保存",class:'btn btn-success' %>
</div>
<% end %>